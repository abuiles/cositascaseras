<%= render :partial => 'store', :locals => { :store => @store, store_page: true} %>

<h1> Cositas </h1>

<% @store.listings.each do |listing| %>
  <%= render :partial => 'listings/listing', :locals => { :listing => listing } %>
<% end %>

<% if @store.admin?(current_user) %>
  <%= link_to 'Agregar Articulo', new_store_listing_path(@store) %>
<% end %>


<%= link_to 'Edit', edit_store_path(@store) %> |
<%= link_to 'Back', stores_path %>
